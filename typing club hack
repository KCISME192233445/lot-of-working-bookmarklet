javascript: delay=prompt("Enter delay (ms). Please use the number 58 or higher and reload your page every time you use it");/*Please keep in mind that this is just an experiment. If your typingclub stuff is graded please do not use this. Seriously*/const rawElements = document.getElementsByClassName("token_unit");const listElements = Array.from(rawElements); const letterList = []; listElements.forEach(item => letterList.push(item.innerHTML.replace(/<\/?[^>]+(>|$)/g, ""))); var strippedLetterList = letterList.map(function(x){return x.trim().replace(/&nbsp;/g, " ");}); for (let i = 0; i < strippedLetterList.length; i++) {setTimeout(function(){window.core.record_keydown_time(strippedLetterList[i])}, delay*i)};
